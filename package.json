{
  "name": "grimvault",
  "version": "1.0.8",
  "description": "GrimVault",
  "author": "DarkerDB",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "build": "node build-gyp.js && vite build && electron-builder",
    "build:dev": "cross-env NODE_ENV=development && node build-gyp.js",
    "dev": "cross-env NODE_ENV=development concurrently \"vite\" \"wait-on tcp:5173 && electron .\"",
    "install": "vcpkg install",
    "postinstall": "electron-builder install-app-deps",
    "publish": "electron-builder --publish always"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "axios": "^1.7.9",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.3.1",
    "electron-dl": "^4.0.0",
    "electron-log": "^5.2.4",
    "electron-updater": "^6.3.9",
    "form-data": "^4.0.2",
    "ini": "^5.0.0",
    "node-addon-api": "^8.3.0",
    "vue": "^3.2.13",
    "winreg": "^1.2.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "node-gyp": "^11.0.0",
    "postcss": "^8.5.2",
    "tailwindcss": "^3.4.17",
    "vite": "^6.1.0",
    "wait-on": "^8.0.2"
  },
  "gypfile": true
}
